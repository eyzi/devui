{
  "name": "devui",
  "description": "Simple UI for a one-click build upload to game platforms",
  "author": "Eyzi",
  "version": "0.1.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/eyzi/devui.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
